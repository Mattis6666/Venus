{
    "name": "venus-ts",
    "version": "1.0.0",
    "main": "out/src/index.js",
    "scripts": {
        "dev": "npm run build && export NODE_ENV=development && npm run start",
        "pm2": "pm2 start --name Neptune npm -- run dev --watch --ignore-watch out/*",
        "production": "npm run build && export NODE_ENV=production && npm run start",
        "test": "tsc -p tsconfig.test.json && npm run lint",
        "lint": "eslint ./src/**/*.ts",
        "push": "npm run gitAdd && npm run gitCommit && git push",
        "clean": "rm -rf out/*",
        "reinstall": "npm run cleanCache && npm run deleteModules && npm run deleteLock && npm run installModules",
        "build": "tsc",
        "start": "node out/src/index",
        "cleanCache": "npm cache clean --force",
        "deleteModules": "rm -rf node_modules/*",
        "deleteLock": "rm yarn.lock",
        "installModules": "yarn install",
        "gitAdd": "git add .",
        "gitCommit": "git commit -m 'owo'"
    },
    "author": "VenNeptury",
    "license": "MIT",
    "husky": {
        "hooks": {
            "pre-commit": "precise-commits && npm run lint"
        }
    },
    "eslintIgnore": [
        "out",
        ".vscode"
    ],
    "dependencies": {
        "@types/bluebird": "^3.5.30",
        "@types/config": "^0.0.36",
        "@types/got": "^9.6.9",
        "@types/mongoose": "^5.7.7",
        "@types/node": "^13.9.8",
        "@types/node-fetch": "^2.5.5",
        "@types/tictactoejs": "^1.0.0",
        "@types/ws": "^7.2.3",
        "bluebird": "^3.7.2",
        "canvas": "^2.6.1",
        "chalk": "^3.0.0",
        "config": "^3.3.0",
        "discord.js": "^12.1.1",
        "discord.js-minesweeper": "^1.0.5",
        "mongoose": "^5.9.6",
        "nekos.life": "^2.0.5",
        "node-fetch": "^2.6.0",
        "ordinal": "^1.0.3",
        "tictactoejs": "^1.0.3",
        "typescript": "^3.8.3",
        "winston": "^3.2.1"
    },
    "devDependencies": {
        "@typescript-eslint/eslint-plugin": "^2.27.0",
        "@typescript-eslint/parser": "^2.27.0",
        "eslint": "^6.8.0",
        "eslint-config-prettier": "^6.10.1",
        "eslint-plugin-prettier": "^3.1.2",
        "husky": "^4.2.3",
        "precise-commits": "^1.0.2",
        "prettier": "^2.0.4"
    }
}
